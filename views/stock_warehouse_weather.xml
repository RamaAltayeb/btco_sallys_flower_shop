<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <record id="stock_warehouse_weather_tree_view" model="ir.ui.view">
            <field name="name">stock_warehouse_weather.tree</field>
            <field name="model">stock.warehouse.weather</field>
            <field name="arch" type="xml">
                <tree create="False">
                    <field name="temperature"/>
                    <field name="pressure"/>
                    <field name="humidity"/>
                    <field name="wind_speed"/>
                    <field name="rain_volume"/>
                    <field name="capture_time"/>
                </tree>
            </field>
        </record>

    </data>
</odoo>